def binom(n,k): #이항계수 top-down
    memo={}
    if n<0 or k<0:
        return 0
    if n<k:
        return 0
    for i in range(1,n):
        memo[(i,1)]=i
        memo[(i,0)]=1
        memo[(i,i)]=1
    for i in range(2,n):
        for j in range(2,i):
            memo[(i,j)]=memo[(i-1,j)]+memo[(i-1,j-1)]
     
    return memo[(n-1,k)]+memo[(n-1,k-1)]
